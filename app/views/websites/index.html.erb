<div id="webJumboTron" class="jumbotron">
      <div class="container">
        <h1>List of Registered Sites</h1>
        <p>This form is used to enter websites into the database. Please be sure that the sites you enter have the html tag used for your parser.</p>
       
     
       <a class="btn btn-default" href="https://open-boot-pachanga.c9users.io/hmbob/display#" role="button">Front Page &raquo;</a> 
       </div>
    </div>
<p id="notice"><%= notice %></p> 






<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-4 col-md-offset-4"></div>
</div>
<div class="row">
  <div class="col-md-3 col-md-offset-3">
  
  <h1>Listing Websites</h1>
  <table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Identifier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @websites.each do |website| %>
      <tr>
        <td><%= website.name %></td>
        <td><%= website.identifier %></td>
        <td><%= link_to 'Show', website %></td>
        <td><%= link_to 'Edit', edit_website_path(website) %></td>
        <td><%= link_to 'Destroy', website, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
  
 <h3> <%= link_to 'New Website', new_website_path %> </h3>
  
  </div>
  <div class="col-md-3 col-md-offset-3"></div>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3"></div>
</div>


